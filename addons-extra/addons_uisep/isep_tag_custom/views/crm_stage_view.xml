<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="inherit_view_crm_stage_form_tag" model="ir.ui.view">
            <field name="name">inherit_view_crm_stage_form_tag</field>
            <field name="model">crm.stage</field>
            <field name="inherit_id" ref="crm.crm_stage_form"/>
            <field name="arch" type="xml">        
                <xpath expr="//field[@name='team_id']" position="after">
                    <field name="tag_crm_id" options="{'no_quick_create': True,'no_create_edit': True}" />
                </xpath>          
            </field>
        </record>
        
    </data>
</odoo>