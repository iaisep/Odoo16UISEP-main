<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <data>
      
        <record id="view_res_company_prematricula_form_view" model="ir.ui.view">
            <field name="name">res.company.prematricula.form</field>
            <field name="model">res.company</field>
            <field name="inherit_id" ref="base.view_company_form"/>  
            <field name="arch" type="xml">
                <xpath expr="//field[@name='parent_id']" position="after">
                    <field name="isep_prematricula_id" domain="[('model', '=', 'sale.order')]" options="{'no_create': True}"/>
                </xpath>
            </field>
        </record>

    </data>
</odoo>
