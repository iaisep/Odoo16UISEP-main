<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="ir_cron_auto_send_library" model="ir.cron">
        <field name="name">Análisis y calificación / Envío a Libreta</field>
        <field name="user_id" ref="base.user_root" />
        <field name="interval_number">6</field>
        <field name="interval_type">hours</field>
        <field name="numbercall">-1</field>
        <field name="doall">1</field>
        <field name="model_id" ref="model_survey_user_input"/>
        <field name="state">code</field>
        <field name="active" eval="True"/>
        <field name="code">model.cron_survey_send_library()</field>
        <field name="nextcall" eval="(datetime.now() + timedelta(minutes=7)).strftime('%Y-%m-%d %H:%M:%S')"/>
    </record>
</odoo>