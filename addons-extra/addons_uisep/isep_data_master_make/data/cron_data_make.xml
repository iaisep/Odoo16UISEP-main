<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="ir_cron_data_master_make" model="ir.cron">
        <field name="name">Datos para Make</field>
        <field name="interval_number">1</field>
        <field name="interval_type">days</field>
        <field name="numbercall">-1</field>
        <field name="doall">1</field>
        <field name="model_id" ref="model_data_master_make"/>
        <field name="state">code</field>
        <field name="active" eval="True"/>
        <field name="code">model.action_send_data()</field>
        <field name="nextcall" eval="(datetime.now() + timedelta(minutes=7)).strftime('%Y-%m-%d %H:%M:%S')"/>
    </record>
</odoo>