<?xml version="1.0" encoding="utf-8"?>
<odoo>
    
    <!-- View account.move form -->
    <record id="view_account_move_form_static" model="ir.ui.view">
        <field name="name">view.account.move.form</field>
        <field name="model">account.move</field>
        <field name="inherit_id" ref="account.view_move_form"/>
        <field name="arch" type="xml">
            <xpath expr="//page[@name='other_info']/group[@id='other_tab_group']/group[@name='accounting_info_group']/field[@name='to_check']" position="after">
                <field name="phone" invisible="1"/>
                <field name="email" invisible="1"/>
            </xpath>
        </field>
    </record>

</odoo>