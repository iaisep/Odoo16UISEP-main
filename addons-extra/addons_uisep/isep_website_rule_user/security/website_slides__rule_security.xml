<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="rule_slide_rule_channel_not_website" model="ir.rule">
            <field name="name">Channel: portal/user: restricted to published and ( user odoo only)</field>
            <field name="model_id" ref="model_slide_channel"/>
            <field name="groups" eval="[(4, ref('base.group_portal')), (4, ref('base.group_user'))]"/>
            <field name="domain_force">['&amp;', ('website_published', '=', True), '|', ('visibility', '=', 'user_odoo'), ('partner_ids', '=', user.partner_id.id)]</field>
            <field name="perm_unlink" eval="0"/>
            <field name="perm_write" eval="1"/>
            <field name="perm_read" eval="1"/>
            <field name="perm_create" eval="0"/>
        </record>
   

    </data>
</odoo>
