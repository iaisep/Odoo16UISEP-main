<?xml version="1.0" encoding="utf-8" ?>
<odoo>
      
    <record id="view_documents_studends_form_inherit" model="ir.ui.view">
        <field name="name">documents.studends.form.inherit</field>
        <field name="model">res.partner</field>
        <field name="inherit_id" ref="base.view_partner_form"/>  
        <field name="arch" type="xml">            
            <xpath expr="//field[@name='vat']" position="after">              
               <field name="have_documents"/>
               <field name="official_title" readonly='1'/>
               <field name="text_missing" invisible='1'/>
               <field name="date_time_next" invisible='1'/>              
               <field name="email_send_status" widget="badge"/>               
            </xpath>
        </field>
    </record>

</odoo>

