<?xml version="1.0" encoding="UTF-8" ?>
<odoo noupdate="1">
    <record id="notice_request_sequence" model="ir.sequence">
        <field name="name">Student Notice</field>
        <field name="code">op.notice</field>
        <field name="prefix">NOTICE</field>
        <field name="padding">6</field>
        <field eval="1" name="number_next"/>
        <field eval="1" name="number_increment"/>
    </record>

    <record id="circular_request_sequence" model="ir.sequence">
        <field name="name">Student Circular</field>
        <field name="code">op.circular</field>
        <field name="prefix">CIR</field>
        <field name="padding">6</field>
        <field name="number_increment">1</field>
    </record>
</odoo>
