<odoo>


        <record id="survey_user_input_line_tree" model="ir.ui.view">
            <field name="name">survey_user_input_line_tree</field>
            <field name="model">survey.user_input.line</field>            
            <field name="inherit_id" ref="survey.survey_response_line_view_tree"/>
            <field name="arch" type="xml">
                <xpath expr="//tree" position="attributes"> 
                    <attribute name="create">0</attribute>
                    <attribute name="delete">0</attribute>
                    <attribute name="edit">0</attribute>
                </xpath>


                <xpath expr="//field[@name='survey_id']" position="before">           
                    <field name="survey_type"  optional="hide"/>                    
                    <field name="course_id"  optional="hide"/>
                    <field name="op_subject_id"  optional="hide"/>
                    <field name="batch_id"  optional="hide"/>
                </xpath>

            </field>
        </record>

        <record id="survey_user_input_line_form" model="ir.ui.view">
            <field name="name">survey_user_input_line_form</field>
            <field name="model">survey.user_input.line</field>            
            <field name="inherit_id" ref="survey.survey_user_input_line_view_form"/>
            <field name="arch" type="xml">
                <xpath expr="//form" position="attributes"> 
                    <attribute name="create">0</attribute>
                    <attribute name="delete">0</attribute>
                    <attribute name="edit">0</attribute>
                </xpath>
            </field>
        </record>







</odoo>
