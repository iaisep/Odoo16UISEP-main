<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="dv_import_data_target_form_view" model="ir.ui.view">
        <field name="name">dv_import_data_target_form_view</field>
        <field name="model">dv.data.import</field>
        <field name="arch" type="xml">
            <form string="Import data target">
                <group>
                    <group>
                        <field name="data_import_dv" string="Upload Json" required="1"/>
                    </group>
                </group>
                <footer>
                    <button name="ks_do_action" string="Import" type="object" class="btn-primary"/>
                    <button string="Cancel" class="btn-secondary" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>


</odoo>