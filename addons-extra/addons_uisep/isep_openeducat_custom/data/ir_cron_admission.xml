<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="ir_cron_update_admission_duration_state" model="ir.cron">
            <field name="name">Actualizar Estado de Grupo/duracion de Admisiones</field>
            <field name="model_id" ref="model_op_admission"/>
            <field name="state">code</field>
            <field name="code">model.sudo()._cron_update_duration_state()</field>
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field> 
            <field name="doall" eval="False"/>
            <field name="nextcall" eval="(DateTime.now() + timedelta(days=1)).strftime('%Y-%m-%d 04:00:00')" /> 
        </record>
    </data>
</odoo>