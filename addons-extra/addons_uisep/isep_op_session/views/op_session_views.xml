<?xml version="1.0" encoding="utf-8"?>
<odoo>
    
    <!-- View op.session form -->
    <record id="view_op_session_make_form" model="ir.ui.view">
        <field name="name">view.op.session.make.form</field>
        <field name="model">op.session</field>
        <field name="inherit_id" ref="openeducat_timetable.view_op_session_form"/>
        <field name="arch" type="xml">
            <xpath expr="//group/group/field[@name='type']" position="after">
                <field name="faculty_name"/>
                <field name="course_name"/>
                <field name="subject_name"/>
                <field name="slide_channel_num"/>
            </xpath>

            <xpath expr="//field[@name='classroom_id']" position="after">
                <field name="start_date" />
            </xpath>
            
        </field>
    </record>

</odoo>